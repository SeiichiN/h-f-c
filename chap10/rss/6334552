var sr  = new YAHOO.i13n.JP.simpleRapid();
var ins = sr.setSpaceid('590602546')
  .setWebWorkerFilePath("/pickup/js/__rapidworker-1.2.js")
  .setPageParam({"service":"news","apptype":"web","opttype":"pc","enc":"utf-8","status":"logout","pagetype":"detail","conttype":"topics","cat_path":"topics,dom","ctsid":"6334552","topittl":"\u897f\u65e5\u672c 27\u65e5\u304b\u3089\u5927\u96e8\u306e\u6050\u308c","topitime":"2019\/8\/26(\u6708) 15:38","topitag":"domestic","topiname":"\u5927\u96e8\u3001\u53f0\u98a8","ispicked":"true","metakwd":"\u30c8\u30d4\u30c3\u30af\u30b9,\u30cb\u30e5\u30fc\u30b9,\u5927\u96e8\u3001\u53f0\u98a8","mit_serv":"news","shcid":"37408735b9045416863e63df7b93e79a97f79b17"})
  .setModule(["tpcHeader","tpcNews","relLink","pageTopWrapper","tpcCmtHd","ranknewsHd","ranknews","rankvideoHd","rankvideo","tpcInfo","timeline","subTopics"])
  .setViewableModule(["viewable-survey","viewable-ycd","viewable-comment"])
  .startViewable()
  .setViewableTime(1000)
  .setCallback(function(){
    var vTag1 = $('<div class="viewableWrap"><div id="viewable-tl1" class="setViewable"><a data-ylk="rsec:v_tpctl;slk:stream;pos:1;"></a></div></div>');
    $('.topicsTl .listFeed').prepend(vTag1);
    $('.topicsTl .listFeedWrap a[data-ylk*="rsec:stream"]').each(function(index, elem){
        if (index > 0 && (index+1) % 5 === 0) {
            var pos = index + 1;
            var vTag = $('<div class="viewableWrap"><div id="viewable-tl' + pos + '" class="setViewable"><a data-ylk="rsec:v_tpctl;slk:stream;pos:' + pos + ';"></a></div></div>');
            $(this).after(vTag);
        }
    });
    ins.addModulesWithViewability(["viewable-tl1","viewable-tl5","viewable-tl10","viewable-tl15","viewable-tl20","viewable-tl25","viewable-tl30","viewable-tl35","viewable-tl40"], false);
  })
  .initRapid();

if (document.getElementById("point_hd")) {
    ins.beaconLinkViews([{
        sec: "point_hd",
        _links: [{
            slk: "tap",
            pos: "1",
            _p: "1"
        }]
    }], false);

    document.getElementById("point_hd").addEventListener("click", function(){
        if(typeof ins !== undefined) {
            ins.beaconClick("point_hd", "tap", "1");
        }
    });
}
