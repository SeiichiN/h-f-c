    function sendBeacon(modName, linksName, sec, slk, spid, vtestid) {
        var link_data = [{
            mod:{name:modName},
            links:[{name:linksName, params:{sec:sec, slk:slk}}]
        }];

        var ins = YAHOO.i13n.Beaconer(
            {beacon_server:'ybx.yahoo.co.jp',
            keys:{vtestid:vtestid,rpdprop:'news'},
            link_data:link_data,
            spaceid:spid,
            https:true
            });
        ins.doClickBeacon(link_data[0].links[0].params);
    }
</script>
