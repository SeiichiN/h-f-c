<script>
function showCommentCount() {
    $.ajax({
        type: 'GET',
        url: 'https://news.yahoo.co.jp/comment/plugin/v1/count/sp_news/20190826-00010294-weather/',
        dataType: 'json',
        data: {
            output: 'json'
        }
    }).done(function(data){
        $("#tpcNews_comment").text(data.count).addClass("tpcNews_comment");
    }).fail(function(data){
        // コメント件数が取得できない場合は0件表示
        $("#tpcNews_comment").text('0').addClass("tpcNews_comment");
    });
}
</script>
