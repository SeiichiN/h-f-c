<script type="text/javascript">
$(function(){
    $('section.tpcHeader-pict').each(function(){
        var $div = $(this);
        var $img = $(this).find('div.tpcHeader_thumb p.tpcHeader_thumb_img');
        var $url = $img.css('background-image').replace('url(','').replace(')','').replace(/\"/gi, "");

        if ($url === void 0) {
            return;
        }

        var imgObj = new Image();
        $(imgObj).one('error',function(){
            $div.find('.tpcHeader_source').remove();
            $div.children('.tpcHeader_thumb').remove();
            $div.addClass('tpcHeader-noPict').removeClass('tpcHeader-pict');
        });

        $(imgObj).one('load',function(){
            var w=imgObj.width,h=imgObj.height,dw=$img.attr('data-width'),dh=$img.attr('data-height');
            if(w > dw || h > dh) {
                if(w/dw > h/dh) $img.css('width',dw+'px');
                else $img.css('height',dh+'px');
            }
            $img.attr('src', $url);
            // 1pix以下は画像が削除されたと判断
            if(w <= 1 || h <= 1) {
                $div.find('.tpcHeader_source').remove();
                $div.children('.tpcHeader_thumb').remove();
                $div.addClass('tpcHeader-noPict').removeClass('tpcHeader-pict');
            }
        });
        imgObj.src = $url;
    });

    // トピ詳細の関連リンク サムネイル デッド対応
    $('li.tpcPoint_item').each(function(){
        var $div = $(this);
        if ($div.find('.tpcPoint_embed-pict').length) {
            var $imgWrap = $div.find('.tpcPoint_embed-pict');
            var $img = $div.find('.tpcPoint_embed-pict img');
        } else if ($div.find('.tpcPoint_thumb').length) {
            var $imgWrap = $div.find('.tpcPoint_thumb');
            var $img = $div.find('.tpcPoint_thumb img');
        } else {
            return;
        }
        var $url = $img.attr('src');

        if ($url === void 0) {
            return;
        }

        var imgObj = new Image();
        $(imgObj).one('error',function(){
            $imgWrap.remove();
        });

        $(imgObj).one('load',function(){
            var w=imgObj.width,h=imgObj.height,dw=$img.attr('data-width'),dh=$img.attr('data-height');
            if(w > dw || h > dh) {
                if(w/dw > h/dh) $img.css('width',dw+'px');
                else $img.css('height',dh+'px');
            }
            $img.attr('src', $url);
            // 1pix以下は画像が削除されたと判断
            if(w <= 1 || h <= 1) {
                $imgWrap.remove();
            }
        });
        imgObj.src = $url;
    });
});
</script>

